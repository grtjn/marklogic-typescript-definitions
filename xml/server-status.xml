<xs:schema targetNamespace="http://marklogic.com/xdmp/status/server" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" xmlns="http://marklogic.com/xdmp/status/server" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:ns2="http://marklogic.com/xdmp/status/request" xmlns:ns="http://marklogic.com/xdmp/status/server" xmlns:hfp="http://www.w3.org/2001/XMLSchema-hasFacetAndProperty" xmlns:admin="http://marklogic.com/xdmp/admin" xmlns:manage="http://marklogic.com/xdmp/manage">
  <xs:simpleType name="server-id">
    <xs:annotation>
      <xs:documentation>A server id.</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="server-id" type="server-id">
    <xs:annotation>
      <xs:documentation>The server id.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="host-id">
    <xs:annotation>
      <xs:documentation>A host unique id (64-bit value).</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="host-id" type="host-id">
    <xs:annotation>
      <xs:documentation>The unique key of the host</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="transaction-id">
    <xs:annotation>
      <xs:documentation>A transaction unique id (64-bit value).</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="transaction-id" type="transaction-id">
    <xs:annotation>
      <xs:documentation>The unique key of the transaction</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="server-status" type="server-status">
    <xs:annotation>
      <xs:documentation>The status of a server</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="server-name">
    <xs:annotation>
      <xs:documentation>A server name.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="server-name" type="server-name">
    <xs:annotation>
      <xs:documentation>The server name.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="server-kind">
    <xs:annotation>
      <xs:documentation>The kind of server</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="http-server"/>
      <xs:enumeration value="xdbc-server"/>
      <xs:enumeration value="odbc-server"/>
      <xs:enumeration value="webDAV-server"/>
      <xs:enumeration value="task-server"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="server-kind" type="server-kind">
    <xs:annotation>
      <xs:documentation>The kind of application server</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="enabled" type="boolean">
    <xs:annotation>
      <xs:documentation>The server is enabled.</xs:documentation>
      <xs:appinfo>
        <manage:health equals="false()" state="maintenance" code="HEALTH-SERVER-DISABLED"/>
        <manage:health equals="false()" state="offline" code="HEALTH-SERVER-OFFLINE"/>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="request-state">
    <xs:annotation>
      <xs:documentation>The state of a request</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="stopped"/>
      <xs:enumeration value="running"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="request-state" type="request-state">
    <xs:annotation>
      <xs:documentation>The state of a request</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="request-kind">
    <xs:annotation>
      <xs:documentation>A kind of request</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="eval"/>
      <xs:enumeration value="invoke"/>
      <xs:enumeration value="get"/>
      <xs:enumeration value="copy"/>
      <xs:enumeration value="delete"/>
      <xs:enumeration value="lock"/>
      <xs:enumeration value="mkcol"/>
      <xs:enumeration value="move"/>
      <xs:enumeration value="options"/>
      <xs:enumeration value="propfind"/>
      <xs:enumeration value="proppatch"/>
      <xs:enumeration value="put"/>
      <xs:enumeration value="unlock"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="request-kind" type="request-kind">
    <xs:annotation>
      <xs:documentation>The kind of request (for example, invoke, eval).</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="port">
    <xs:annotation>
      <xs:documentation>An integer internet port number, min 1, max 65534.</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="65534"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="port" type="port">
    <xs:annotation>
      <xs:documentation>The server socket bind internet port number.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="ssl-certificate-template-id">
    <xs:annotation>
      <xs:documentation>
        The id of a certificate template in the security database.
      </xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="ssl-certificate-template" type="ssl-certificate-template-id">
    <xs:annotation>
      <xs:documentation>
        The certificate template.
      </xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="directory-pathname">
    <xs:annotation>
      <xs:documentation>It can be absolute or relative.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="root" type="directory-pathname">
    <xs:annotation>
      <xs:documentation>The root document directory pathname.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="database-id">
    <xs:annotation>
      <xs:documentation>A database ID.</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="modules" type="database-id">
    <xs:annotation>
      <xs:documentation>The ID of the database that contains application modules. If on the file-system, then 0.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="database" type="database-id">
    <xs:annotation>
      <xs:documentation>The ID of the content database when applicable. In some cases, this is a forest ID or a random number (when neither a database ID nor a forest ID is available).</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="last-login" type="database-id">
    <xs:annotation>
      <xs:documentation>The ID of the last login database.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="display-last-login" type="boolean">
    <xs:annotation>
      <xs:documentation>Whether an appserver should display last-login information.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="address">
    <xs:annotation>
      <xs:documentation>A numeric internet address.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:pattern value="[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="address" type="address">
    <xs:annotation>
      <xs:documentation>The server socket bind numeric internet address.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="backlog">
    <xs:annotation>
      <xs:documentation>An integer number of connections, min 1, max 4096.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="4096"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="backlog" type="backlog">
    <xs:annotation>
      <xs:documentation>The socket listen backlog.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="threadCount">
    <xs:annotation>
      <xs:documentation>An integer number of threads, min 0, max 256.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="256"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="threads" type="threadCount">
    <xs:annotation>
      <xs:documentation>The current number of active applicatiton server threads.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="max-threads" type="threadCount">
    <xs:annotation>
      <xs:documentation>The maximum number of application server threads allowed.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="concurrent-request-limit" type="xs:integer">
    <xs:annotation>
      <xs:documentation>The concurrent request limit.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="timeout">
    <xs:annotation>
      <xs:documentation>An integer number of seconds, min 0, max 4294967295.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="4294967295"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="memory-size">
    <xs:annotation>
      <xs:documentation>
        An integer amount of system memory in megabytes, min 1, max 32768.
      </xs:documentation>
      <xs:appinfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="32768"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="request-timeout" type="timeout">
    <xs:annotation>
      <xs:documentation>The request socket recv timeout, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="keep-alive-timeout" type="timeout">
    <xs:annotation>
      <xs:documentation>The keep-alive socket recv timeout, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="session-timeout" type="timeout">
    <xs:annotation>
      <xs:documentation>The session expiration timeout, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="static-expires" type="timeout">
    <xs:annotation>
      <xs:documentation>The static content expiration interval, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="max-time-limit" type="timeout">
    <xs:annotation>
      <xs:documentation>The upper bound for a request's time limit, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="time-limit" type="timeout">
    <xs:annotation>
      <xs:documentation>The request's time limit, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="default-time-limit" type="timeout">
    <xs:annotation>
      <xs:documentation>The default time limit for a request, in seconds.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="max-inference-size" type="memory-size">
    <xs:annotation>
      <xs:documentation>The upper bound for a request's inference size, in megabytes.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="default-inference-size" type="memory-size">
    <xs:annotation>
      <xs:documentation>The default inference size for a request, in megabytes.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="collation">
    <xs:annotation>
      <xs:documentation>
        A collation.
      </xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:element name="collation" type="collation">
    <xs:annotation>
      <xs:documentation>
        The default collation for queries.
      </xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="authentication">
    <xs:annotation>
      <xs:documentation>The authentication mode for the server.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="application-level"/>
      <xs:enumeration value="basic"/>
      <xs:enumeration value="digest"/>
      <xs:enumeration value="digestbasic"/>
      <xs:enumeration value="kerberos-ticket"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="authentication" type="authentication">
    <xs:annotation>
      <xs:documentation>The authentication scheme to use for this server</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="multi-version-concurrency-control">
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="contemporaneous"/>
      <xs:enumeration value="nonblocking"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="multi-version-concurrency-control" type="multi-version-concurrency-control"/>
  <xs:simpleType name="distribute-timestamps">
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="fast"/>
      <xs:enumeration value="strict"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="distribute-timestamps" type="distribute-timestamps"/>
  <xs:simpleType name="user">
    <xs:annotation>
      <xs:documentation>A user ID.</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="default-user" type="user">
    <xs:annotation>
      <xs:documentation>The user used as the default user in application level authentication. Using the admin user as the default
      user is equivalent to turning security off.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="privilege-id">
    <xs:annotation>
      <xs:documentation>A privilege to restrict access to a server.</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="privilege" type="privilege-id">
    <xs:annotation>
      <xs:documentation>The privilege restricting access to the server.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="pathname">
    <xs:annotation>
      <xs:documentation>It can be absolute or relative.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="path" type="pathname">
    <xs:annotation>
      <xs:documentation>The path to a queery</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="text">
    <xs:annotation>
      <xs:documentation>A query/request text</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="request-text" type="xs:string">
    <xs:annotation>
      <xs:documentation>The request text.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="request-rewritten-text" type="text">
    <xs:annotation>
      <xs:documentation>The rewritten request text.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="start-time">
    <xs:annotation>
      <xs:documentation>A Label version number.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:dateTime"/>
  </xs:simpleType>
  <xs:element name="start-time" type="start-time">
    <xs:annotation>
      <xs:documentation>The time the operation started.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="trigger-depth">
    <xs:annotation>
      <xs:documentation>An integer number of trigger invocation.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="trigger-depth" type="trigger-depth">
    <xs:annotation>
      <xs:documentation>The maximum depth of trigger invocation.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="user" type="user">
    <xs:annotation>
      <xs:documentation>The unique ID of the user executing the request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="boolean">
    <xs:annotation>
      <xs:documentation>true or false</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:boolean"/>
  </xs:simpleType>
  <xs:element name="update" type="boolean">
    <xs:annotation>
      <xs:documentation>Specifies if this request an update (true for update requests).</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="request-id">
    <xs:annotation>
      <xs:documentation>An id for a request</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="request-id" type="request-id">
    <xs:annotation>
      <xs:documentation>A unique id for a request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="canceled" type="boolean">
    <xs:annotation>
      <xs:documentation>Has this request been canceled.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="client-address">
    <xs:annotation>
      <xs:documentation>An address of a client</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:element name="client-address" type="client-address">
    <xs:annotation>
      <xs:documentation>The network address of a client</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="profiling-allowed" type="boolean">
    <xs:annotation>
      <xs:documentation>Is profiling allowed for this request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="profiling-enabled" type="boolean">
    <xs:annotation>
      <xs:documentation>Is profiling currently enabled for this request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="debugging-allowed" type="boolean">
    <xs:annotation>
      <xs:documentation>Is debugging allowed for this request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="debugging-status">
    <xs:annotation>
      <xs:documentation>A request's debugging status.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:element name="debugging-status" type="debugging-status">
    <xs:annotation>
      <xs:documentation>The debugging status of the request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="expr-id">
    <xs:annotation>
      <xs:documentation>An expression identifier.</xs:documentation>
      <xs:appinfo>
        <manage:no-units/>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="expr-id" type="expr-id">
    <xs:annotation>
      <xs:documentation>The expression id.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="where-stopped">
    <xs:annotation>
      <xs:documentation>Where in an expression a request is stopped.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="begin"/>
      <xs:enumeration value="end"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="where-stopped" type="where-stopped">
    <xs:annotation>
      <xs:documentation>Where the request is stopped.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="request-status">
    <xs:annotation>
      <xs:documentation>A set of request statuses.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="request-id"/>
      <xs:element ref="server-id"/>
      <xs:element ref="host-id"/>
      <xs:element ref="transaction-id"/>
      <xs:element ref="canceled"/>
      <xs:element ref="modules"/>
      <xs:element ref="database"/>
      <xs:element minOccurs="0" ref="root"/>
      <xs:element ref="request-kind"/>
      <xs:element ref="request-text"/>
      <xs:element minOccurs="0" ref="request-rewritten-text"/>
      <xs:element ref="update"/>
      <xs:element ref="start-time"/>
      <xs:element ref="time-limit"/>
      <xs:element ref="max-time-limit"/>
      <xs:element ref="user"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="client-address"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="trigger-depth"/>
      <xs:element ref="expanded-tree-cache-hits"/>
      <xs:element ref="expanded-tree-cache-misses"/>
      <xs:element ref="request-state"/>
      <xs:element ref="profiling-allowed"/>
      <xs:element ref="profiling-enabled"/>
      <xs:element ref="debugging-allowed"/>
      <xs:element ref="debugging-status"/>
      <xs:element ref="retry-count"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="expr-id"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="where-stopped"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="request-status" type="request-status">
    <xs:annotation>
      <xs:documentation>The request being processed by the server.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="request-statuses">
    <xs:annotation>
